<%= will_paginate @spinoffs %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th><h3>Parent Name</h3></th>
        <th><h3>Parent Ticker</h3></th>
        <th><h3>Spinoff Name</h3></th>
        <th><h3>Spinoff Ticker</h3></th>
        <th><h3>Spinoff Date</h3></th>
      </tr>
    </thead>
    <tbody>
      <% @spinoffs.each do |spin| %>
        <tr>
          <td><%= spin.parent_name %></td>
          <td><%= link_to spin.parent_ticker, controller: 'spinoffs', action: 'show', id: spin %></td>   
          <td><%= spin.spinoff_name %></td>
          <td><%= link_to spin.spinoff_ticker, controller: 'spinoffs', action: 'show', id: spin  %></td>
          <td><%= spin.spinoff_date %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<%= will_paginate @spinoffs %>